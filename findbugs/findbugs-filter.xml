<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <Match>
        <!-- ignore all issues in resource generation -->
        <Class name="~.*\.R\$.*"/>
    </Match>
    <Match>
        <!-- ignore all issues in auto-generated Manifest files -->
        <Class name="~.*\.Manifest\$.*"/>
    </Match>
</FindBugsFilter>